<section class="block">
  <%= render 'shared/admin_tabs' %>
  <section class="content">            
    <h2 class="title"><%= t("web-app-theme.new", :default => "New")%>  Role</h2>
    <section class="inner">
      <%= form_for :role, :url => roles_path, :html => { :class => :form } do |f| -%>
